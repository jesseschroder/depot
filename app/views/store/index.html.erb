<% if notice %>
  <aside id="notice"><%= notice %></aside>
<% end %>

<h1>Your Pragmatic Catalog</h1>
<!--<div class="Polaris-Layout">-->
<!--  <div class="Polaris-Layout__Section">-->
<ul class="catalog">
    <% cache @products do %>
      <% @products.each do |product| %>
        <% cache product do %>
          <%= render 'product_details', product: product %>
          <% end %>
      <% end %>
    <% end %>
</ul>
